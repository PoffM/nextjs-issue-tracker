{
  "name": "nextjs-issue-tracker",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build:1-migrate": "prisma migrate deploy",
    "build:2-build": "next build",
    "build": "run-s build:*",
    "db-up": "docker-compose up -d && npx wait-on tcp:5832",
    "db-seed": "prisma db seed",
    "db-migrate-dev": "npx prisma migrate dev",
    "db-nuke": "docker-compose down --volumes --remove-orphans",
    "dev": "next dev",
    "dx": "run-s db-up db-migrate-dev db-seed dev",
    "start": "next start",
    "lint": "eslint src",
    "lint-fix": "npx lint --fix",
    "test": "jest",
    "test-dev": "start-server-and-test dev 3000 test:e2e",
    "test-start": "start-server-and-test start 3000 test:e2e",
    "test:e2e": "npm run db-seed && npx playwright test",
    "postinstall": "prisma generate",
    "prepare": "husky install",
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\""
  },
  "prisma": {
    "seed": "npm run ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.4",
    "@prisma/client": "^4.3.1",
    "@rehooks/local-storage": "^2.4.4",
    "@tanstack/react-table": "^8.5.13",
    "@trpc/client": "^9.27.2",
    "@trpc/next": "^9.27.2",
    "@trpc/react": "^9.27.2",
    "@trpc/server": "^9.27.2",
    "clsx": "^1.2.1",
    "focus-trap-react": "^10.0.0",
    "lodash": "^4.17.21",
    "next": "12.3.0",
    "next-auth": "^4.10.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.36.1",
    "react-icons": "^4.4.0",
    "react-intersection-observer": "^9.4.0",
    "react-query": "^3.39.2",
    "react-select": "^5.4.0",
    "react-textarea-autosize": "^8.3.4",
    "superjson": "^1.10.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.25.2",
    "@types/lodash": "^4.14.185",
    "@types/node": "18.7.18",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "autoprefixer": "^10.4.11",
    "daisyui": "^2.28.0",
    "eslint": "^8.23.1",
    "eslint-config-next": "^12.3.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.8",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.25.2",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "prisma": "^4.3.1",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.1",
    "typescript": "4.8.3",
    "wait-on": "^6.0.1"
  }
}
